<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="242dp"
        android:layout_height="31dp"
        android:layout_marginTop="360dp"
        android:gravity="left|center_vertical"
        android:text="FECHADURA DIGITAL DE EMBUTIR"
        android:textColor="#000000"
        android:textSize="12"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.177"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.173" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="241dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="320dp"
        android:fontFamily="sans-serif"
        android:text="FDE-300W"
        android:textColor="#444444"
        android:textSize="24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="330"
        android:layout_height="320"
        app:srcCompat="@drawable/fde300w"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="-9dp" />

    <LinearLayout
        android:layout_width="178dp"
        android:layout_height="73dp"
        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="412dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:srcCompat="@drawable/img1" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:gravity="bottom|clip_horizontal|clip_vertical|right|fill|fill_horizontal|top"
            android:text="Resistente à água"
            android:textColor="#444444" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="178dp"
        android:layout_height="76dp"
        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="493dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="60"
            android:layout_height="60"
            app:srcCompat="@drawable/img2" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="118dp"
            android:layout_height="32dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="Acesso remoto via wi-fi"
            android:textColor="#444444" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="178dp"
        android:layout_height="76dp"
        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="579dp">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="60"
            android:layout_height="60"
            app:srcCompat="@drawable/img3" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:gravity="center"
            android:text="Teclado numérico"
            android:textColor="#444444" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="178dp"
        android:layout_height="73dp"
        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="412dp">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="60"
            android:layout_height="60"
            app:srcCompat="@drawable/img4" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="124dp"
            android:layout_height="36dp"
            android:gravity="center"
            android:text="Compatível com Alexa"
            android:textColor="#444444" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="178dp"
        android:layout_height="73dp"
        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="579dp">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="60"
            android:layout_height="60"
            app:srcCompat="@drawable/img6" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:gravity="center"
            android:text="Acesso por cartão"
            android:textColor="#444444" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="178dp"
        android:layout_height="73dp"
        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="493dp">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="60"
            android:layout_height="60"
            app:srcCompat="@drawable/img5" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Leitor Biométrico"
            android:textColor="#444444" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="409dp"
        android:layout_height="262dp"
        android:gravity="center"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="398dp"></LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="296dp"
        android:layout_height="43dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#444444"
        android:textColor="#F4F2F2"
        app:cornerRadius="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Comprar agora" />

</androidx.constraintlayout.widget.ConstraintLayout>